<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        app:layout_constraintLeft_toRightOf="@+id/imageView"
        tools:text="Duraton"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:textSize="45sp"
        tools:text="0:00:00"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        />

    <ImageView
        android:layout_width="23dp"
        android:layout_height="23dp"
        app:srcCompat="@drawable/ic_alarm_black_24dp"
        android:id="@+id/imageView"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"/>

    <android.support.constraint.Guideline
        android:layout_width="384dp"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="117dp"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintGuide_begin="117dp"/>

    <ImageView
        android:layout_width="23dp"
        android:layout_height="23dp"
        app:srcCompat="@drawable/ic_pin_drop_black_24dp"
        android:id="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/guideline"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/imageView2"
        android:layout_marginLeft="8dp"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="16dp"
        app:layout_constraintGuide_begin="16dp"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="16dp"
        android:id="@+id/textView4"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        android:textSize="45sp"
        tools:text="0.00"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView4"
        tools:text="mile"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline3"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="240dp"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintGuide_percent="0.46966732"/>

    <ImageView
        app:srcCompat="@drawable/speedometer_icon"
        android:id="@+id/imageView3"
        android:layout_height="16dp"
        android:layout_width="16dp"
        app:layout_constraintTop_toTopOf="@+id/guideline3"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        android:layout_marginTop="24dp"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="39dp"
        android:id="@+id/textView6"
        tools:text="Pace"
        tools:layout_editor_absoluteY="271dp"/>

    <ImageView
        android:layout_height="16dp"
        app:srcCompat="@drawable/fire"
        android:id="@+id/imageView4"
        android:layout_width="16dp"
        tools:layout_editor_absoluteY="272dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        tools:text="Calories"
        tools:layout_editor_absoluteY="272dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/imageView4"
        android:layout_marginLeft="8dp"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        android:layout_marginTop="16dp"
        tools:text="-"
        android:textSize="18sp"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        app:layout_constraintLeft_toRightOf="@+id/textView8"
        android:textSize="12sp"
        app:layout_constraintVertical_bias="1.0"
        tools:text="min/mile"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView8"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        tools:text="0"
        android:textSize="18sp"
        android:textStyle="normal|bold"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        app:layout_constraintLeft_toRightOf="@+id/textView10"
        tools:text="kcal"
        android:textSize="12sp"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView10"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline4"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="206dp"
        app:layout_constraintGuide_begin="206dp"/>

    <ImageView
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:srcCompat="@drawable/running"
        tools:layout_editor_absoluteX="16dp"
        android:id="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/guideline5"/>

    <TextView
        android:text="Running"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/imageView5"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline5"
        app:layout_constraintGuide_begin="329dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="329dp"
        tools:layout_editor_absoluteX="0dp"/>

    <ImageView
        app:srcCompat="@drawable/green_circle"
        android:id="@+id/imageView6"
        android:layout_width="92dp"
        android:layout_height="93dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/guideline6"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_play_arrow_white_24dp"
        android:id="@+id/imageView7"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/guideline6"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintRight_toRightOf="@+id/imageView6"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_fiber_manual_record_black_24dp"
        tools:layout_editor_absoluteY="430dp"
        tools:layout_editor_absoluteX="73dp"
        android:id="@+id/imageView8"/>

    <TextView
        android:text="10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:textColor="#ffffff"
        app:layout_constraintTop_toTopOf="@+id/imageView8"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintRight_toRightOf="@+id/imageView8"
        app:layout_constraintLeft_toLeftOf="@+id/imageView8"/>

    <TextView
        android:text="Basic Workout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        app:layout_constraintTop_toTopOf="@+id/guideline6"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        android:layout_marginTop="8dp"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline6"
        app:layout_constraintGuide_begin="385dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="385dp"
        tools:layout_editor_absoluteX="0dp"/>

    <TextView
        android:text="GPS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"/>

    <ImageView
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:srcCompat="@drawable/ic_remove_circle_black_24dp"
        android:id="@+id/imageView9"
        tools:layout_editor_absoluteX="241dp"
        app:layout_constraintTop_toTopOf="@+id/textView15"
        app:layout_constraintBottom_toBottomOf="@+id/textView15"/>
</android.support.constraint.ConstraintLayout>
